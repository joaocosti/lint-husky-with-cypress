{
    "env": {
      "es2021": true,
      "node": true
    },
    "extends": [
      "airbnb-base",
      "plugin:cypress/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "cypress/no-force": "error",
      "cypress/no-pause": "error"
    },
    "plugins": [
      "cypress"
    ]
  }
  